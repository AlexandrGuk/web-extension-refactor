{
  "manifest_version": 3,
  "version": "1.0",
  "name": "Old New Tab",
  "description": "Default new tab take over with incredible background",
  "chrome_url_overrides": {
      "newtab": "index.html"
  },
  "icons": {
      "128": "icon.png"
  },
  "permissions": ["storage", "unlimitedStorage"],
  "background": {
    "service_worker": "js/bg.js"
  }
}
